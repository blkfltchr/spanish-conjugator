"generated":{"line":121,"column":15},"source":"components/Verb/Input.js","original":{"line":82,"column":15}}],"sources":{"components/Verb/Input.js":"import React, { Component } from 'react';\nimport PropTypes from 'prop-types';\nimport Person from './Person';\nimport AccentButtons from './AccentButtons';\nimport Checkmark from '../Checkmark/Checkmark';\n\nclass Input extends Component {\n  render() {\n    const {\n      randomPerson,\n      value,\n      handleChange,\n      correct,\n      addAccent,\n      handleExample,\n      helperText,\n      handleSubmit,\n      buttonText,\n    } = this.props;\n    return (\n      <div>\n        <form onSubmit={this.handleSubmit}>\n          <div className=\"input-section\">\n            <Person randomPerson={randomPerson[0]} />\n            <input\n              type=\"text\"\n              defaultValue={value}\n              placeholder=\"Enter conjugated verb...\"\n              onChange={handleChange}\n              className=\"input\"\n            />\n            <Checkmark correct={correct} />\n          </div>\n          <div className=\"text-under-input\">\n            <AccentButtons addAccent={addAccent} />\n            <div\n              className=\"hover-text\"\n              type=\"button\"\n              role=\"button\"\n              tabIndex={0}\n              onClick={handleExample}\n            >\n              Show example <i className=\"far fa-arrow-alt-circle-right\" />\n            </div>\n          </div>\n          <div className=\"helper-text\">\n            {helperText && <div>{helperText}</div>}\n            <button\n              className=\"submit-button\"\n              type=\"submit\"\n              onClick={handleSubmit}\n            >\n              {buttonText}\n            </button>\n          </div>\n        </form>\n      </div>\n    );\n  }\n}\n\nInput.propTypes = {\n  randomPerson: PropTypes.array,\n  value: PropTypes.string,\n  handleChange: PropTypes.func,\n  correct: PropTypes.bool,\n  addAccent: PropTypes.func,\n  handleExample: PropTypes.func,\n  helperText: PropTypes.string,\n  handleSubmit: PropTypes.func,\n  buttonText: PropTypes.string,\n};\n\nInput.defaultProps = {\n  randomPerson: ['answer', 'answer'],\n  value: '',\n  correct: false,\n  helperText: '',\n  buttonText: '',\n};\n\nexport default Input;\n"},"lineCount":null}},"error":null,"hash":"1927a179b7a7cb620aa9fe067ec02284","cacheData":{"env":{}}}